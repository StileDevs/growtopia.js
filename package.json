{
  "name": "growtopia.js",
  "version": "2.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "bun": "./index.js",
      "import": "./esm.mjs",
      "require": "./index.js"
    }
  },
  "napi": {
    "name": "growtopia-js",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "armv7-unknown-linux-musleabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin",
        "riscv64gc-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.1",
    "@hono/node-server": "^1.13.7",
    "@napi-rs/cli": "^2.18.4",
    "@types/node": "^22.9.0",
    "ava": "^6.0.1",
    "hono": "^4.6.10",
    "rimraf": "5.0.10",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "homepage": "https://github.com/StileDevs/growtopia.js#readme",
  "bugs": {
    "url": "https://github.com/StileDevs/growtopia.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StileDevs/growtopia.js.git"
  },
  "keywords": [
    "growtopia",
    "rust",
    "enet"
  ],
  "author": "JadlionHD <jadlion.dev@gmail.com>",
  "scripts": {
    "docs": "rimraf docs && typedoc",
    "artifacts": "napi artifacts",
    "build": "nr build:ts && napi build --platform --js native.js --dts native.d.ts --release",
    "build:ts": "rimraf dist && tsc",
    "build:debug": "napi build --platform --js native.js --dts native.d.ts ",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  }
}
